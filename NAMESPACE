import('qeML')
import('regtools')
import('gtools')
import('ranger')

export(
   getSuggestedLib,
   dsldFreqPCoord,
   dsldTakeALookAround, dsldConditDisparity, dsldUtilFairness,
   dsldScatterPlot3D,
   dsldLinear, dsldLogit, dsldGetData, dsldCheckData, dsldDiffSLog, dsldDiffSLin,
   dsldConfounders, dsldDensityByS, dsldFrequencyByS,
   dsldFgrrm, dsldFrrm, dsldNclm, dsldQeFairRidgeLin, dsldQeFairRF, 
   dsldQeFairRidgeLog, dsldQeFairKNN, dsldML, dsldCHunting, dsldFairTest
)

S3method(summary, dsldLM)
S3method(coef, dsldLM)
S3method(vcov, dsldLM)

S3method(summary, dsldGLM)
S3method(coef, dsldGLM)
S3method(vcov, dsldGLM)

S3method(predict, dsldQeFairRidgeLin)
S3method(predict, dsldQeFairRidgeLog)
S3method(predict, dsldQeFairRF)
S3method(predict, dsldQeFairKNN)
